{
  "name": "universal-react-tutorial",
  "version": "0.0.0",
  "description": "A tutorial on building a universal React web application.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "start": "webpack && node server.js",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "webpack -w",
    "watch:server": "nodemon --ext js,jsx --ignore public/ server.js"
  },
  "main": "server.js",
  "keywords": [
    "universal",
    "isomorphic",
    "react",
    "tutorial"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "bcryptjs": "^2.4.3",
    "bloomer": "^0.6.0",
    "body-parser": "^1.17.2",
    "bulma": "^0.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "express": "^4.13.4",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.1",
    "mailgun-js": "^0.11.2",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "react-script-loader": "0.0.1",
    "redux": "^3.6.0",
    "sendgrid": "^5.1.2",
    "sendgrid-nodejs": "0.1.0-2",
    "stripe": "^4.23.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2"
  },
  "private": true,
  "license": "MIT"
}
